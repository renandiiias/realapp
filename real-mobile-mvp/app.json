{
  "expo": {
    "name": "Real",
    "slug": "real-mobile-mvp",
    "scheme": "realapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-real.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-real.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0C0E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-real.png",
        "backgroundColor": "#0B0C0E"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita acesso para enviar vídeos de até 15 segundos.",
          "cameraPermission": "Permita acesso à câmera para gravar vídeos de até 15 segundos.",
          "microphonePermission": "Permita acesso ao microfone para gravar vídeo com áudio."
        }
      ]
    ]
  }
}
